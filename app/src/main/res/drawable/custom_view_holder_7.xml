<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">

            <gradient android:startColor="#4f4ed7" android:centerColor="#f55a9b" android:endColor="#4f4ed7" android:type="linear"/>
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">

            <solid android:color="@color/transparent"/> />
        </shape>

    </item>

</layer-list>
